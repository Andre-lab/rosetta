cd %(workdir)s
[ -x %(bin)s/rosetta_scripts.%(binext)s ] || exit 1
%(bin)s/rosetta_scripts.%(binext)s @flags -database %(database)s -run:constant_seed -nodelay  2>&1 \
    | egrep -vf ../../ignore_list \
    > log

