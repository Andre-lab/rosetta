cd %(workdir)s

[ -x %(bin)s/ligands_to_database.%(binext)s ] || exit 1
%(bin)s/ligands_to_database.%(binext)s @flags_param_output -database %(database)s -run:constant_seed -nodelay  2>&1 \
    | egrep -vf ../../ignore_list \
    > log

[ -x %(bin)s/rosetta_scripts.%(binext)s ] || exit 1
%(bin)s/rosetta_scripts.%(binext)s @flags_dock -database %(database)s -run:constant_seed -nodelay  2>&1 \
    | egrep -vf ../../ignore_list \
    >> log


echo hi > result
