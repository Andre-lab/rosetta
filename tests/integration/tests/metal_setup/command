cd %(workdir)s

[ -x %(bin)s/fixbb.%(binext)s ] || exit 1
%(bin)s/fixbb.%(binext)s %(additional_flags)s @flags -database %(database)s -run:constant_seed -nodelay  2>&1 \
    | egrep -vf ../../ignore_list \
    > log

# test 1l8c readin
%(bin)s/fixbb.%(binext)s %(additional_flags)s -s 1l8c.pdb -resfile repack_1l8c.res -in:auto_setup_metals -database %(database)s -run:constant_seed -nodelay  2>&1 \
    | egrep -vf ../../ignore_list \
    > log

test "${PIPESTATUS[0]}" != '0' && exit 1 || true  # Check if the first executable in pipe line return error and exit with error code if so
