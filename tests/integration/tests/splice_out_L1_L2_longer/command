
cd %(workdir)s

%(bin)s/rosetta_scripts.%(binext)s %(additional_flags)s @flags -parser:script_vars source_pdb=%(database)s/protocol_data/splice/antibodies/template_pdbs/2BRR.ppk.pdb -s %(database)s/protocol_data/splice/antibodies/template_pdbs/2BRR.ppk.pdb -database %(database)s -testing:INTEGRATION_TEST -run:jran 11113 2>&1 \
    | egrep -v 'seconds' \
    | egrep -v 'core.init'\
    | egrep -vf ../../ignore_list \
    > log

test "${PIPESTATUS[0]}" != '0' && exit 1 || true  # Check if the first executable in pipe line return error and exit with error code if so


