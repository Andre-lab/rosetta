cd %(workdir)s
[ -x %(bin)s/gen_lig_grids.%(binext)s ] || exit 1
%(bin)s/gen_lig_grids.%(binext)s %(additional_flags)s @flags -database %(database)s -testing:INTEGRATION_TEST  2>&1 \
    | egrep -vf ../../ignore_list \
    > log

test "${PIPESTATUS[0]}" != '0' && exit 1 || true  # Check if the first executable in pipe line return error and exit with error code if so
