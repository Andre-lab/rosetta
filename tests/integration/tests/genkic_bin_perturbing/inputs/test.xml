				<AddResidue index=17 />
				<AddResidue index=18 />
				<AddResidue index=1 />
				<AddResidue index=2 />
				<AddResidue index=3 />
			</AddPerturber>
			<AddFilter type=loop_bump_check />
		</GeneralizedKIC>

		<PDBTrajectoryRecorder name=record1 stride=1 filename=traj.pdb cumulate_jobs=1 cumulate_replicas=1 />

		<ParsedProtocol name=mc_moves1 >
			<Add mover=genkic2 />
			<Add mover=record1 />
		</ParsedProtocol>
		
		<GenericMonteCarlo name=mc1 trials=100 mover_name=mc_moves1 scorefxn_name=tala drift=1 preapply=0 />

	
	</MOVERS>
	<APPLY_TO_POSE>
	</APPLY_TO_POSE>
	<PROTOCOLS>
		<Add mover=build1 />
		<Add mover=tors1 />
		<Add mover=bond1 />
		<Add mover=close1 />
		<Add mover=mc1 />
	</PROTOCOLS>
	<OUTPUT scorefxn=tala />
</ROSETTASCRIPTS>
