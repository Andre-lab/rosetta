	<SCOREFXNS>
		<tala weights="talaris2014.wts" />
	</SCOREFXNS>
	<TASKOPERATIONS>
		<RestrictToRepacking name=repack_only />
	</TASKOPERATIONS>
	<FILTERS>
	</FILTERS>
	<MOVERS>

		<PeptideStubMover name=build1 reset=true>
			<Append resname="ALA" />
			<Append resname="ALA" />
			<Append resname="ALA" />
			<Append resname="ALA" />
			<Append resname="ALA" />
			<Append resname="ALA" />
