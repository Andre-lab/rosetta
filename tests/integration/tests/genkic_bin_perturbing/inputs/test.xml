				<AddResidue index=16 />
				<AddResidue index=17 />
				<AddResidue index=18 />
				<AddResidue index=1 />
			</AddPerturber>
			<AddFilter type=loop_bump_check />
		</GeneralizedKIC>

		<GeneralizedKIC name=genkic2 selector=lowest_rmsd_selector closure_attempts=2000 stop_when_solution_found=true >
			<AddResidue res_index=5 />
			<AddResidue res_index=6 />
			<AddResidue res_index=7 />
			<AddResidue res_index=8 />
			<AddResidue res_index=9 />
