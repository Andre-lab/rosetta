<dock_design>
         <TASKOPERATIONS>
    		<InitializeFromCommandline name=init/>
		<SeqprofConsensus name=seqprofcons min_aa_probability=2 conservation_cutoff_protein_interface_design=0 conservation_cutoff_aligned_segments=1 probability_larger_than_current=0 ignore_pose_profile_length_mismatch=1> 
				<ProteinInterfaceDesign design_chain1=1 design_chain2=0/> 
				<RestrictToAlignedSegments chain=1>
					<L1 source_pdb="%%source_pdb%%" start_res=24 stop_res=41/>
					<L2 source_pdb="%%source_pdb%%" start_res=51 stop_res=60/>
					<L3 source_pdb="%%source_pdb%%" start_res=94 stop_res=105/>
					<H1 source_pdb="%%source_pdb%%" start_res=135 stop_res=147/>
					<H2 source_pdb="%%source_pdb%%" start_res=155 stop_res=171/>
					<H3 source_pdb="%%source_pdb%%" start_res=209 stop_res=220/>
				</RestrictToAlignedSegments>
			</SeqprofConsensus>
  	</TASKOPERATIONS>
        <SCOREFXNS>
		<TalarisCal_res_type_cst weights=talaris2013_calibrated>
			<Reweight scoretype="res_type_constraint" weight=0.5/>
		</TalarisCal_res_type_cst>
        </SCOREFXNS>

        <FILTERS>
	        <DesignableResidues name=designable task_operations=seqprofcons/>
        </FILTERS>

        <MOVERS>
		<Splice name=splice_seqconstraints scorefxn=TalarisCal_res_type_cst add_sequence_constraints_only=1 protein_family=antibodies segment=L3/>
        </MOVERS>

        <APPLY_TO_POSE>
        </APPLY_TO_POSE>
        <PROTOCOLS>
                <Add mover=splice_seqconstraints/>
                <Add filter=designable/>
        </PROTOCOLS>
</dock_design>
