# change to working directory
cd %(workdir)s

# check to see if executable exits
 [ -x %(bin)s/UnfoldedStateEnergyCalculator.%(binext)s ] || exit 1

# run with flags for random fragments
%(bin)s/UnfoldedStateEnergyCalculator.%(binext)s %(additional_flags)s @flags_random -database %(database)s -run:constant_seed -nodelay  2>&1 \
     | egrep -vf ../../ignore_list \
     > log_random

# run with flags for sequence matched fragments
%(bin)s/UnfoldedStateEnergyCalculator.%(binext)s %(additional_flags)s @flags_sequence_match -database %(database)s -run:constant_seed -nodelay  2>&1 \
     | egrep -vf ../../ignore_list \
     > log_sequence_match

# test to see if exe terminated abnormaly
test "${PIPESTATUS[0]}" != '0' && exit 1 || true
