cd %(workdir)s

[ -x %(bin)s/cst_info.%(binext)s ] || exit 1 
%(bin)s/cst_info.%(binext)s %(additional_flags)s @commandline_flags -database %(database)s -run:constant_seed -nodelay -no_prof_info_in_silentout 2>&1 \
    | egrep -vf ../../ignore_list \
    > log_commandline

test "${PIPESTATUS[0]}" != '0' && exit 1 || true  # Check if the first executable in pipe line return error and exit with error code if so

[ -x %(bin)s/rosetta_scripts.%(binext)s ] || exit 1 
%(bin)s/rosetta_scripts.%(binext)s %(additional_flags)s @rosetta_scripts_flags -database %(database)s -run:constant_seed -nodelay -no_prof_info_in_silentout 2>&1 \
    | egrep -vf ../../ignore_list \
    > log_rosetta_scripts

test "${PIPESTATUS[0]}" != '0' && exit 1 || true  # Check if the first executable in pipe line return error and exit with error code if so

