cd %(workdir)s

# Do the tests actually exist?
[ -x %(bin)s/glycan_relax.%(binext)s ] || exit 1

%(bin)s/glycan_relax.%(binext)s %(additional_flags)s @flags -database %(database)s \
    -run:constant_seed -nodelay 2>&1 | egrep -vf ../../ignore_list > log \
test "${PIPESTATUS[0]}" != '0' && exit 1 || true