# change to working directory
cd %(workdir)s

# check to see if executable exits
 [ -x %(bin)s/MakeRotLib.%(binext)s ] || exit 1

# run with norvaline flags
%(bin)s/MakeRotLib.%(binext)s %(additional_flags)s @flags_nvl -database %(database)s -run:constant_seed -nodelay  2>&1 \
     | egrep -vf ../../ignore_list \
     > log_nvl

# test to see if exe terminated abnormaly
test "${PIPESTATUS[0]}" != '0' && exit 1 || true

# run with Nspe flags
%(bin)s/MakeRotLib.%(binext)s %(additional_flags)s @flags_601 -database %(database)s -run:constant_seed -nodelay  2>&1 \
     | egrep -vf ../../ignore_list \
     > log_601

# test to see if exe terminated abnormaly
test "${PIPESTATUS[0]}" != '0' && exit 1 || true
